# I - Noitcereon - will put any custom path modifications in here.
# Command to add to path is:
# export PATH=/some-path:$PATH
# Make note of the : after some-path and the $ on the second mention of PATH.

# Javahome/tandersen25/.jdks/temurin-21.0.6
JAVA_21_HOME="/home/tandersen25/.jdks/temurin-21.0.6"
JAVA_17_HOME="/usr/lib/jvm/java-17-openjdk-amd64"
export JAVA_HOME="${JAVA_17_HOME}"
export PATH="${JAVA_HOME}/bin:${PATH}"

# rider.sh
RIDER_BIN_HOME="/home/noitcereon/Applications/JetBrains Rider-2023.3.2/bin"
export PATH="${RIDER_BIN_HOME}:${PATH}"

# dotnet
DOTNET_HOME="/home/noitcereon/.dotnet"
export PATH="${DOTNET_HOME}:${PATH}"

# node
# Node  was instead added to PATH via NVM in .bashrc (automatically via script)
# export PATH="/home/noitcereon/ManuallyInstalledDependencies/node-v20.11.0-linux-x64/bin":$PATH

# code (Visual Studio Code)
export PATH="/usr/share/code/code":$PATH

# IntelliJ IDEA
INTELLIJ_BIN_HOME="/home/noitcereon/Applications/idea-IU-232.9921.47/bin"
export PATH="${INTELLIJ_HOME}:${PATH}"



